
page{
		background: url(data:image/jpeg;base64,);
}
.content{
		height: 100%;
		width: 100%;
}
.top-header{
		position: fixed;
		top: 0;
		width: 100%;
		height: 215rpx;
		overflow: hidden;
		z-index: 1000;
		background-color: #CEE3F6;
}
.detail-weeks{
		text-align: center;
		width: 100%;
		/* margin-left: 38%; */
		height: 60rpx;
		line-height: 60rpx;
		font-size: 40rpx;
}
.term{
		margin-left: 38%;
		font-size: 28rpx;
		width: 24%;
}
.up-down{
		width: 40rpx;
		height: 40rpx;
		line-height: 50rpx;
}
.mask-container{
		width: 100%;
		height: 240rpx;
		background-color: #FFFFFF;
		position: fixed;
		bottom: 0;
}
.mask{
		width: 100%;
		height: 240rpx;
		z-index: 100;
		position: fixed;
		bottom: 0;
}
.pick-view{
		text-align: center;
		width: 100%;
        height: 240rpx;
		line-height: 80rpx;
		 text-overflow: hidden;
		font-size: 40rpx;
}
.weekday{
		display:-webkit-box;
		display:-webkit-flex;
		display:flex;
		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-webkit-flex-direction: row;
		        flex-direction: row;
		-webkit-flex-wrap: wrap;
		        flex-wrap: wrap;
		text-align: center;
		font-size: 32rpx;
}
.days{
		margin-top: 20rpx;
		/* text-align: center; */
		width: 86rpx;
		height: 90rpx;
		line-height: 90rpx;
		background-color: #FFFFFF;
		margin-left: 8rpx;
		border-radius: 10rpx;
}
.month{
		display: inline-block;
		width: 80rpx;
		height: 90rpx;
		text-align: center;
		line-height: 90rpx;
		font-size: 28rpx;
		margin-top: 20rpx;
}
.current{
		background-color: #CCCCFF;
}
.everyday-class{
		width: 40rpx;
		padding-left: 10rpx;
		margin-top: 220rpx;
}
.total{
		width: 80rpx;
		line-height: 80rpx;
		vertical-align: middle;
		height: 200rpx;
		background-color: #4CD964;
}
.detail{
		text-align: center;
		width: 80rpx;
		/* height: 40rpx; */
		margin-top: 15rpx;
		font-size: 32rpx;
}
.jie-times{
		text-align: center;
		width: 80rpx;
		height: 40rpx;
		font-size: 28rpx;
}

